              audio(src="http://stream.suayan.com.s3.amazonaws.com/audio/Pisay90_2015_Closure-Mix.mp3", class="album-cover video-js vjs-default-skin",poster=config.mediaHost+"/images/pisay90/badge-blue-inverse.png",controls="controls",preload="auto",data-setup='{}')
